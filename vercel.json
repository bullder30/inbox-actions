{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "prisma generate && prisma db push && next build",
  "crons": [
    {
      "path": "/api/cron/daily-sync",
      "schedule": "43 16 * * *"
    },
    {
      "path": "/api/cron/cleanup-metadata",
      "schedule": "40 16 * * *"
    }
  ]
}
